<policies>
    <inbound>
        <base />

        <return-response>
            <set-status code="200" />
            <set-body>@(context.Request.Url.QueryString)</set-body>
        </return-response>
    </inbound>

    <backend>
        <base />
    </backend>

    <outbound>
        <base />
    </outbound>

    <on-error>
        <base />
    </on-error>
</policies>